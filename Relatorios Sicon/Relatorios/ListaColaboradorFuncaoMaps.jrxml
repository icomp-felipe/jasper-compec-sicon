		<parameter name="TABLE_CONCURSO" class="java.lang.Integer"/>
		<parameter name="TABLE_GROUP" class="java.lang.String"/>
		<parameter name="TABLE_FUNCAO" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select c.colab_nome, i.inst_codigo, i.inst_nome, i.inst_bairro, i.inst_municipio_id, m.muni_nome, i.inst_maps
from inscricao inscr
join colaborador c on inscr.idColaborador = c.colab_id_pk
join instituicao i on inscr.idinstituicaoopcao1 = i.inst_id_pk
join municipio m on i.inst_municipio_id = m.muni_id_pk
where inscr.idconcurso = $P{TABLE_CONCURSO}
  and inscr.idFuncao = $P{TABLE_FUNCAO}
  and i.inst_id_pk <> 4
order by $P!{TABLE_GROUP}]]>
		</queryString>
