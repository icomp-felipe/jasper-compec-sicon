		<parameter name="TABLE_GROUP" class="java.lang.String"/>
		<parameter name="TABLE_FUNCAO" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select c.nome as COLAB_NOME, i.inst_codigo as INST_COD, i.inst_nome as INST_NOME, i.inst_bairro as INST_BAIRRO, i.inst_maps as INST_MAPS
from colaborador c, instituicao i, inscricao inscr
where inscr.idconcurso = $P!{TABLE_CONCURSO}
  and inscr.idFuncao   = $P!{TABLE_FUNCAO}
  and inscr.idColaborador = c.idColaborador
  and inscr.idinstituicaoopcao1 = i.inst_id_pk
  and i.inst_codigo <> 1
order by $P!{TABLE_GROUP}]]>
		</queryString>
