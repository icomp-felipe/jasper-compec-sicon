		<queryString>
			<![CDATA[select c.nome as NOME, substring(i.nome,1,3) as CODIGO, substring(i.nome,5) as INSTITUICAO
from colaborador c, instituicao i, inscricao j
where c.idColaborador=j.idColaborador and i.idinstituicao=j.idinstituicaoopcao1
and j.idconcurso=$P!{TABLE_CONCURSO} and j.idinstituicaoopcao1 <> 4 and j.idFuncao=4
group by $P!{TABLE_GROUP}]]>
		</queryString>
		<field name="NOME" class="java.lang.String"/>
